</head>
<body>
<jsp:include page="header_sidebar.jsp" />
   <div class="content-body">
<div class="card-body">
                                
                            
          
                       <div class="col-lg-8">
                       <%
                                             Connection cn1=new DBUtil().getDatabaseConnection();
                                             PreparedStatement stfirst = cn1.prepareStatement("select * from feedback");
                                         ResultSet rs1=stfirst.executeQuery();
                                         while(rs1.next())
                                         {
%>
                                <div class="card border-warning">
                               
                                    <div class="card-header"><%=rs1.getString("fullname") %></div>
                                    <div class="card-body">
                                        <h5 class="card-title"><%=rs1.getString("subject") %></h5>
                                        <p class="card-text"><%=rs1.getString("comment") %></p>
                                    </div>
                                    
                                </div>
                                <%}cn1.close(); %>
                            </div>
                            </div>
                            
                    
       <!--**********************************
        Scripts
    ***********************************-->
    <script src="plugins/common/common.min.js"></script>
    <script src="js/custom.min.js"></script>
